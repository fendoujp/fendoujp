RewriteEngine on

RewriteRule ^index.html$ index.php

RewriteRule ^([a-zA-Z]{1,})-list-([0-9]+).html$ /index.php?a=article&t=$1&class_id=$2
RewriteRule ^news-view-([0-9]+).html$ /index.php?a=detail&t=news&id=$1
RewriteRule ^photo-view-([0-9]+).html$ /index.php?a=detail&t=photo&id=$1
RewriteRule ^school-view-([0-9]+).html$ /index.php?a=detail&t=school&id=$1
RewriteRule ^([a-zA-Z]{1,})-([0-9]+)-([0-9]+).html$ /index.php?a=article&t=$1&class_id=$2&pageId=$3